         <v-toolbar color=\"primary\" dark flat>\n                    <v-toolbar-title>{{ $t(\"auth.Login\") }}</v-toolbar-title>\n                    <v-spacer></v-spacer>\n                    <v-tooltip bottom>\n                      <template v-slot:activator=\"{ on }\">\n                        <v-btn icon large target=\"_blank\" v-on=\"on\">\n                          <v-toolbar-title>\n                            <router-link style=\"color: 'white'\" to=\"/signup\">\n                              <span>{{ $t(\"auth.Signup\") }}</span>\n                            </router-link>\n                          </v-toolbar-title>\n                        </v-btn>\n                      </template>\n                      <span>New Account</span>\n                    </v-tooltip>\n                  </v-toolbar>\n                  <v-card-text>\n                    <v-form>\n                      <v-text-field\n                        :label=\"$t('auth.Email')\"\n                        name=\"login\"\n                        prepend-icon=\"mdi-email\"\n                        type=\"text\"\n                        v-model=\"email\"\n                      ></v-text-field>\n                      <v-text-field\n                        id=\"password\"\n                        :label=\"$t('auth.Password')\"\n                        name=\"password\"\n                        prepend-icon=\"mdi-lock\"\n                        type=\"password\"\n                        v-model=\"password\"\n                      ></v-text-field>\n                    </v-form>\n                  </v-card-text>\n                  <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn\n                      color=\"primary\"\n                      @click.prevent=\"login\"\n                      class=\"login-btn\"\n                      >Login</v-btn\n                    >\n                  </v-card-actions>\n                </v-card>\n\n                <v-row>\n                  <v-col offset=\"3\">\n                    <span @click=\"arabic\">\n                      <country-flag country=\"sy\" size=\"big\" />\n                    </span>\n                    <span @click=\"english\">\n                      <country-flag country=\"gb\" size=\"big\" />\n                    </span>\n                  </v-col>\n                </v-row>\n              </v-col>\n            </v-row>\n          </v-container>\n        </v-main>\n      </v-app>\n    </v-app>\n  </div>\n</template>\n\n<script>\nimport CountryFlag from \"vue-country-flag\";\n\nexport default {\n  data: function () {\n    return {\n      email: \"\",\n      password: \"\",\n      langs: [\"en\", \"ar\"],\n    };\n  },\n  methods: {\n    login() {\n      this.$store.dispatch(\"login\", {\n        email: this.email,\n        password: this.password,\n      });\n    },\n    arabic() {\n      this.$i18n.locale = \"ar\";\n    },\n    english() {\n      console.log(\"11\", this.$i18n.locale);\n      this.$i18n.locale = \"en\";\n    },\n  },\n  components: {\n    CountryFlag,\n  },\n};\n</script>\n\n<style scoped>\n.login-btn {\n  margin-right: 50%;\n  width: 100%;\n}\n\n.v-application a {\n  margin-right: 40px;\n  color: white;\n}\n</style>"]}]}